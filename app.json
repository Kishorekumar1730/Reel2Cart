{
  "expo": {
    "name": "Reel2Cart",
    "slug": "Reel2Cart",
    "scheme": "reel2cart",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/simple-logo.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kishokumar1730.reel2cart"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-logo-sq.png",
        "backgroundColor": "#000000"
      },
      "package": "com.kishokumar1730.reel2cart",
      "edgeToEdgeEnabled": true,
      "usesCleartextTraffic": true,
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {},
    "plugins": [
      "@react-native-community/datetimepicker",
      "expo-video",
      "expo-notifications",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Reel2Cart to use your location."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "f9ab8afe-9fd9-4369-b68e-2aa58b586951"
      }
    }
  }
}