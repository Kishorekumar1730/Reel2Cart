{
  "expo": {
    "name": "Reel2Cart",
    "slug": "Reel2Cart",
    "scheme": "reel2cart",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kishokumar1730.reel2cart"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000"
      },
      "package": "com.kishokumar1730.reel2cart",
      "edgeToEdgeEnabled": true,
      "usesCleartextTraffic": true
    },
    "web": {},
    "extra": {
      "eas": {
        "projectId": "29e92592-7f24-4061-b552-321312384912"
      }
    },
    "plugins": [
      "@react-native-community/datetimepicker",
      "expo-video",
      "expo-notifications",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Reel2Cart to use your location."
        }
      ]
    ]
  }
}